{
  "env": {
    "browser": true,
    "jquery": true,
    "es6": true,
    "node": true,
    "amd": true
  },
  "globals": {
    "w2ui": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-console": 0,
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ]
  },
  "parserOptions": {
    "ecmaVersion": 2017
  }
}
